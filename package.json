{
  "name": "nemo-ccd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "watch": {
    "test": "src/**/*.js",
    "build": "src/**/*.js"
  },
  "scripts": {
    "start": "node index.js",
    "build": "./node_modules/.bin/browserify --standalone Physics src/index.js > dist/physics.js && ./node_modules/.bin/uglifyjs dist/physics.js > dist/physics.min.js",
    "watch-build": "./node_modules/.bin/npm-watch build",
    "test": "tape 'src/**/*.test.js' | tap-spec"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "npm-watch": "^0.5.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "uglify-es": "^3.3.9",
    "express": "^4.16.4"
  },
  "keywords": [
    "physics",
    "collision",
    "non-elastic",
    "momentumless",
    "circle2circle",
    "sensor"
  ],
  "author": "Viljami Peltola",
  "license": "MIT"
}
